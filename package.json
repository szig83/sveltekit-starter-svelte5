{
	"name": "sveltekit-starter-svelte5",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:e2e": "playwright test",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"update:dep:latest": "ncu --target latest -ui --format group --install always --packageManager bun",
		"update:dep:newest": "ncu --target newest -ui --format group --install always --packageManager bun"
	},
	"devDependencies": {
		"@fontsource-variable/inter": "^5.0.18",
		"@fontsource-variable/oswald": "^5.0.20",
		"@inlang/paraglide-js": "1.9.1",
		"@inlang/paraglide-js-adapter-sveltekit": "^0.6.7",
		"@playwright/test": "^1.44.1",
		"@sveltejs/adapter-auto": "^3.2.1",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.5.10",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/better-sqlite3": "^7.6.10",
		"@types/eslint": "^8.56.10",
		"@types/node": "^20.12.13",
		"@types/ua-parser-js": "^0.7.39",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@typescript-eslint/parser": "^7.11.0",
		"autoprefixer": "^10.4.19",
		"clsx": "^2.1.1",
		"daisyui": "^4.11.1",
		"drizzle-kit": "^0.22.1",
		"eslint": "^9.3.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.0",
		"npm-check-updates": "^16.14.20",
		"open-props": "1.7.4",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-css-order": "^2.1.2",
		"prettier-plugin-svelte": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.6.1",
		"svelte": "^5.0.0-next.148",
		"svelte-check": "^3.8.0",
		"tailwind-merge": "^2.3.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"ua-parser-js": "^1.0.38",
		"vite": "^5.2.12",
		"vitest": "^1.6.0",
		"winston": "^3.13.0"
	},
	"dependencies": {
		"better-sqlite3": "^11.0.0",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.31.0"
	},
	"type": "module",
	"packageManager": "bun@1.0.33",
	"engines": {
		"node": ">=20"
	}
}
